{
  "$shema": "https://json.schemastore.org/package.json",
  "name": "@inqludeit/qualweb-plugin-cmp",
  "version": "0.0.0-development",
  "description": "Qualweb plugin to handle/dismiss \"cookie banners\" (i.e. CMPs).",
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "./dist/index.ts",
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "config": {
    "npm_config_puppeteer_skip_download": true
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "main": "dist/src/index.js",
    "types": "dist/src/index.d.ts",
    "exports": {
      ".": {
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
      },
      "./*": {
        "default": "./dist/*.js",
        "types": "./dist/*.d.ts"
      }
    }
  },
  "scripts": {
    "test": "pnpm test:unit",
    "test:unit": "mocha",
    "test:coverage": "nyc mocha",
    "ts-run": "node -r ts-node/register",
    "build": "pnpm tsc --build -v && npx rimraf dist/test",
    "build:force": "pnpm tsc --build -f -v",
    "clean": "pnpm tsc --build --clean",
    "prepublishOnly": "pnpm build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/inqludeit/inqludeit-backend.git",
    "directory": "packages/cmp"
  },
  "author": "Johannes L. Borresen <jlb@inqludeit.dk>",
  "license": "ISC",
  "dependencies": {
    "@qualweb/types": "^0.7.20",
    "glob": "^8.0.3",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "puppeteer": "13.5.1",
    "tslib": "^2.4.1"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@tsconfig/recommended": "^1.0.1",
    "@types/chai": "^4.3.4",
    "@types/glob": "^8.0.0",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.188",
    "@types/mocha": "^10.0.0",
    "@types/node": "16",
    "chai": "^4.3.7",
    "http-server": "^14.1.1",
    "jquery": "^3.6.1",
    "mocha": "^10.1.0",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  },
  "release": {
    "repositoryUrl": "https://github.com/inqludeit/qualweb-plugin-cmp.git",
    "branches": [
      "main",
      "next",
      {
        "name": "development",
        "prerelease": true
      }
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular"
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "angular"
        }
      ],
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md",
          "changelogTitle": "Changelog"
        }
      ]
    ]
  }
}
